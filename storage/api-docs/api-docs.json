{
    "openapi": "3.0.0",
    "info": {
        "title": "TruckPag Product Parser API",
        "description": "Documentação da API de produtos TruckPag",
        "version": "1.0.0"
    },
    "paths": {
        "/api/products": {
            "get": {
                "tags": [
                    "Produtos"
                ],
                "summary": "Lista todos os produtos",
                "operationId": "1bfaa78d1c2c3848ab8165c5dadcad3e",
                "parameters": [
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Número da página",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 1
                        }
                    },
                    {
                        "name": "per_page",
                        "in": "query",
                        "description": "Quantidade de itens por página",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 100
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Lista de produtos retornada com sucesso"
                    }
                }
            }
        },
        "/api/products/{code}": {
            "get": {
                "tags": [
                    "Produtos"
                ],
                "summary": "Exibe um produto específico",
                "operationId": "d3deebb6679ed9b4ecd1c3af94736c15",
                "parameters": [
                    {
                        "name": "code",
                        "in": "path",
                        "description": "Código do produto",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Produto retornado com sucesso"
                    }
                }
            },
            "put": {
                "tags": [
                    "Produtos"
                ],
                "summary": "Atualiza um produto",
                "operationId": "0f1e54a84587dee40efb8d8341a15536",
                "parameters": [
                    {
                        "name": "code",
                        "in": "path",
                        "description": "Código do produto",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "status": {
                                        "type": "string",
                                        "default": "published"
                                    },
                                    "url": {
                                        "type": "string",
                                        "default": "https://example.com"
                                    },
                                    "creator": {
                                        "type": "string",
                                        "default": "John Doe"
                                    },
                                    "product_name": {
                                        "type": "string",
                                        "default": "Product Name"
                                    },
                                    "quantity": {
                                        "type": "string",
                                        "default": "100g"
                                    },
                                    "brands": {
                                        "type": "string",
                                        "default": "brand1, brand2"
                                    },
                                    "categories": {
                                        "type": "string",
                                        "default": "category1, category2"
                                    },
                                    "labels": {
                                        "type": "string",
                                        "default": "label1, label2"
                                    },
                                    "cities": {
                                        "type": "string",
                                        "default": "city1, city2"
                                    },
                                    "purchase_places": {
                                        "type": "string",
                                        "default": "purchase_place1, purchase_place2"
                                    },
                                    "stores": {
                                        "type": "string",
                                        "default": "store1, store2"
                                    },
                                    "ingredients_text": {
                                        "type": "string",
                                        "default": "ingredient1, ingredient2"
                                    },
                                    "traces": {
                                        "type": "string",
                                        "default": "trace1, trace2"
                                    },
                                    "serving_size": {
                                        "type": "string",
                                        "default": "100g"
                                    },
                                    "serving_quantity": {
                                        "type": "number",
                                        "default": 100
                                    },
                                    "nutriscore_score": {
                                        "type": "number",
                                        "default": 100
                                    },
                                    "nutriscore_grade": {
                                        "type": "string",
                                        "default": "A"
                                    },
                                    "main_category": {
                                        "type": "string",
                                        "default": "main_category"
                                    },
                                    "image_url": {
                                        "type": "string",
                                        "default": "https://example.com/image.jpg"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "204": {
                        "description": "No content"
                    }
                }
            }
        }
    },
    "tags": [
        {
            "name": "Produtos",
            "description": "Produtos"
        }
    ]
}